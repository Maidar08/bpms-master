{
  "name": "erin-bpms-web-client",
  "version": "4.10.0",
  "private": true,
  "engines": {
    "node": "14.20"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "production-proxy": "ng serve --configuration=production-proxy --proxy-config proxy.prod.conf.json",
    "standalone-proxy": "ng serve --configuration=standalone-proxy --proxy-config proxy.stdln.conf.json",
    "build-prod": "ng build --prod --base-href=./ --configuration=production --outputPath=dist/prod",
    "build-standalone": "ng build --base-href=./ --outputPath=dist/standalone",
    "teamcity": "ng test --reporters=teamcity --browsers=CustomLauncher",
    "test-coverage": "ng test --no-watch --code-coverage",
    "test": "ng test --watch=true --browsers=Chrome",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "~9.2.1",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/forms": "~9.1.13",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^8.6.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bpmn-js": "^5.0.5",
    "buffer": "^6.0.3",
    "file-type": "^16.5.4",
    "html2pdf.js": "^0.9.2",
    "karma-teamcity-reporter": "^1.1.0",
    "material-design-icons": "^3.0.1",
    "pdfjs-dist": "2.3.200",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "^9.1.13",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.55",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "puppeteer": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-angular": "^3.0.3",
    "typescript": "~3.8.3",
    "typescript-tslint-plugin": "^1.0.2"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
